default :
{
    name : "openssl"
    get_source : "native"
    compil_script :  $name + $VARS.scriptExtension
    system_info : 
    {
        rpm : []
        rpm_dev : ["openssl-devel"]
        apt : []
        apt_dev : ["libssl-dev"]
    }
    environ :
    {
       env_script : $name + ".py"
    }
    depend : []
    properties :
    {
      incremental : "yes"
    }
}

